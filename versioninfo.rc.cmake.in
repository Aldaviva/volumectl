#include <winver.h>
//#include <ntdef.h>

1 TYPELIB "versioninfo.rc"

VS_VERSION_INFO VERSIONINFO
FILEVERSION 0,0,0,0
PRODUCTVERSION 0,0,0,0 //unused
FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
  FILEFLAGS VS_FF_DEBUG
#else
  FILEFLAGS 0x0L
#endif
FILEOS VOS_NT
FILETYPE VFT_APP
FILESUBTYPE VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Ben Hutchison"
            VALUE "FileDescription", "VolumeCtl"
            VALUE "FileVersion", "0.0.0" //unused
            VALUE "InternalName", "volumectl.exe"
            VALUE "LegalCopyright", "© 2023 Ben Hutchison"
            VALUE "OriginalFilename", "volumectl.exe"
            VALUE "ProductName", "VolumeCtl"
            VALUE "ProductVersion", "0.0.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END